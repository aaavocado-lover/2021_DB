<h1>{{ Airplane 삭제 }}</h1>
<table>
    <tr>
        <td>Airplane_id</td>
        <td>Total_number_of_seats</td>
        <td>Airplane_type</td>
    </tr>

    {{#each Airplane}}
        <form method="post">
            <tr>
                <td>{{Airplane_id}}</td>
                <td>{{Total_number_of_seats}}</td>
                <td>{{Airplane_typE}}</td>
                <td style="border:none; margin-left: 10px;">
                    <button
                        style="margin-left: 10px;"
                        type="submit"
                        name="delBtn"
                        value={{Airplane_id}}
                        formaction="/airplane_delete">
                        삭제
                        </button>
                </td>
            </tr>
        </form>
    {{/each}}
</table>