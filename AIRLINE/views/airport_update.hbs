<h1>{{ Airport 수정 }}</h1>
<table>
    <tr>
        <td>Airport_code</td>
        <td>Name</td>
        <td>City</td>
        <td>State</td>
    </tr>

    {{#each Airport_res}}
        <form method="post" action='/manager/airport'>
            <tr>
                <!-- <td>{{Airport_code}}</td> -->
                <td><input type="text" name="code" placeholder={{Airport_code}}</td>
                <td>{{Name}}</td>
                <td>{{City}}</td>
                <td>{{State}}</td>
                <td><input type="submit" value="수정" formaction="/manager/airport"</td>
            </tr>        
        </form>
    {{/each}}
</table>